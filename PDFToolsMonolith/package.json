{
  "name": "pdf-tools-monolith",
  "version": "1.0.0",
  "private": true,
  "proxy": "http://localhost:4000",
  "dependencies": {
    "axios": "^1.7.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.26.1",
    "react-scripts": "^5.0.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "start:server": "npm --prefix server run start",
    "dev": "concurrently -n client,server -c green,blue \"npm:start\" \"npm --prefix server run dev\"",
    "build": "react-scripts build",
    "postinstall": "npm --prefix server install",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "migrate": "npm --prefix server run migrate"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3"
  },
  "overrides": {
    "react-router-dom": "^6.26.1"
  }
}
